{
  "name": "persjourn",
  "version": "1.0.0",
  "description": "A website for my personal journal entries",
  "main": "server/server.js",
  "scripts": {
      "start": "node server/server.js --ignore client",
      "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
      "install": "cd server && npm i && cd ../client && npm i",
      "seed": "cd server && npm run seed",
      "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AaronDreyer/PersJourn.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "bugs": {
    "url": "https://github.com/AaronDreyer/PersJourn/issues"
  },
  "homepage": "https://github.com/AaronDreyer/PersJourn#readme",
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "apollo-server-express": "^3.12.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "react-router-dom": "^6.12.0",
    "tailwindcss": "^3.3.3"
  }
}
